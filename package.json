{
  "name": "areact",
  "version": "0.0.1-alpha",
  "description": "React-like UI programming in Dao3.fun(Arena)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "node": "./dist/index.cjs",
      "default": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components.mjs",
      "require": "./dist/components.cjs",
      "node": "./dist/components.cjs",
      "default": "./dist/components.cjs",
      "types": "./dist/components.d.ts"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/*"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/color": "^3.0.6",
    "color": "^4.2.3",
    "css-select": "^5.1.0",
    "cssom": "^0.5.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-d.ts": "^1.3.0",
    "htm": "^3.1.1",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^9.1.0",
    "typescript": "^5.6.2",
    "uhyphen": "^0.2.0"
  },
  "dependencies": {
    "preact": "^10.24.0"
  },
  "scripts": {
    "test": "esbuild test.tsx --bundle --minify --legal-comments=none --outfile=testout.js",
    "esbuild:build": "node ./esbuild.mjs",
    "build": "npm run esbuild:build",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}