{
  "name": "areact",
  "version": "0.0.1-alpha",
  "description": "React-like UI programming in Dao3.fun(Arena)",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/esm/components.js",
      "require": "./dist/cjs/components.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/*"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/color": "^3.0.6",
    "color": "^4.2.3",
    "css-select": "^5.1.0",
    "cssom": "^0.5.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-d.ts": "^1.3.0",
    "htm": "^3.1.1",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^9.1.0",
    "typescript": "^5.6.2",
    "uhyphen": "^0.2.0"
  },
  "dependencies": {
    "preact": "^10.24.0"
  },
  "scripts": {
    "test": "esbuild test.tsx --bundle --minify --legal-comments=none --outfile=testout.js",
    "esbuild:build": "node ./esbuild.mjs",
    "build": "npm run esbuild:build",
    "prepublishOnly": "npm run build",
    "pub":"npm publish --registry=https://registry.npmjs.org",
    "pub-gh":"npm publish --registry=https://npm.pkg.github.com/ --scope=@Box3TRC @Box3TRC/areact"
  }
}
